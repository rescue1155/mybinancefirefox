{
  "name": "My Binance Order Notification",
  "version": "1.1",
  "description": "My Binance Order Notification",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "64": "icons/64.png",
    "128": "icons/128.png"
  },
  "permissions": [
    "notifications", "storage", "https://api.binance.com/"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "manifest_version": 2,
  "web_accessible_resources": [
    "48.png"
  ],
  "browser_action": {
    "default_icon": "icons/128.png",
    "default_title": "MyBinance",
    "default_popup": "popup.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "mybinancefirefox@mozilla.org",
      "strict_min_version": "57.0a1"
    }
  }
}